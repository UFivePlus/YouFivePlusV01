<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <gradient
        android:startColor="#D4AF37"
        android:endColor="#F5E770"
        android:angle="45">
    </gradient>
    <corners
        android:radius="120dp"/>
    <padding
        android:bottom="10pt"
        android:left="6pt"
        android:right="10pt"
        android:top="7pt">
    </padding>
    <stroke
        android:color="#FBFBFD"
        android:width="5dp">
    </stroke>

</shape>